{

  "name": "@doctopia/ngx-paypal",
  "version": "9.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/steffenkuche/ngx-paypal"
  },
  "publishConfig": {
    "registry": "https://repo.repsy.io/npm/doctopia/default/"
  },
  "author": "richard sustek",
  "description": "Paypal integration for Angular",
  "keywords": [
    "Paypal",
    "Angular paypal",
    "Paypal integration angular",
    "Ngx paypal",
    "Paypal SDK",
    "Paypal javascript"
  ],
  "private": false,
  "scripts": {
    "ng": "cd ../../ && npm run ng",
    "build:lib": "npm run ng -- build ngx-paypal-lib",
    "publish:lib": "npm run build:lib && cd dist && npm publish",
    "publish:lib:beta": "npm run build:lib && npm publish dist --tag=next",
    "prepublishOnly": "npm run preventPublish",
    "preventPublish": "node prevent-publish"
  },
  "peerDependencies": {
    "@angular/common": "^13.3.9",
    "@angular/core": "^13.3.9"
  }
}
